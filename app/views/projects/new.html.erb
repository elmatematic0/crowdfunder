<h1>Create Your Project</h1>

<%= form_for @project do |f| %>

  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>

  <div class="field">
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </div>

  <div class="field">
    <%= f.label :goal %><br />
    <%= f.number_field :goal %>
  </div>

  <!-- Need to figure out date pickers for start_data and end_date Note as of: Dec 3 -->
  <div class="field">
    <%= f.label :start_date %><br />
    <%= f.date_select :start_date %>
  </div>


  <div class="field">
    <%= f.label :end_date %><br />
    <%= f.date_select :end_date %>
  </div>



   <div class="field">
      <%= f.submit %>
  </div>

<% end %>





